{
  "name": "svg2ico",
  "version": "1.0.26",
  "description": "Convert SVG files to ICO format",
  "keywords": [
    "svg",
    "ico",
    "convert",
    "ico generator",
    "svg to ico",
    "icon file"
  ],
  "bin": {
    "svg2ico": "dist/src/svg2ico.mjs",
    "svg2ico-setup": "dist/src/setup-config.js",
    "svg2ico-update": "dist/src/svg2ico-update.js",
    "svg2ico-resize": "dist/src/resize-config.js"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "setup": "node dist/src/setup-config.js",
    "update": "node dist/src/setup-config.js --update",
    "set-size": "node dist/src/setup-config.js --set-size",
    "resize": "node dist/src/resize-config.js"
  },
  "dependencies": {
    "@shopify/cli-kit": "^3.76.0",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.33.5",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "bugs": {
    "url": "https://github.com/visyoss/SVG-to-ICO-converter/issues"
  },
  "homepage": "https://github.com/visyoss/SVG-to-ICO-converter#readme",
  "author": "Ethan",
  "license": "MIT",
  "readme": "README.npm.md"
}
